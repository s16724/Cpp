#include<iostream>
#include<conio.h>

using namespace std;

int main()
{
	int n, i;
	cout << "Podaj liczbe calkowita n\n n=";
	cin >> n;
	while (n==0)
	{
		cout << "bledne dane podaj wartosc calkowita inna od 0 \n n=";
		cin >> n;
	} 
	cout << "liczby podzielne przez " << n << " to: ";
	for (i = 1; i <= n; i++)
	{
		if (n%i == 0) cout <<" "<< i;
	}
	
	_getch();
}